<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.supernurse.ui.profile.ProfileFragment"
        android:label="@string/label_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_records"
        android:name="com.example.supernurse.ui.records.RecordsFragment"
        android:label="@string/label_records"
        tools:layout="@layout/fragment_records" />

    <fragment
        android:id="@+id/navigation_invoices"
        android:name="com.example.supernurse.ui.invoices.InvoicesFragment"
        android:label="@string/label_invoices"
        tools:layout="@layout/fragment_invoices" />
</navigation>